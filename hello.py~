from flask import Flask
app = Flask(__name__)

@app.route('/')
def hello_world():
    return 'Hello World!'

if __name__ == '__main__':
    app.run(host='0.0.0.0')

@app.route('/hello')
def hello():
    return 'Hello everyone'

@app.route('/user/<username>')
def show_user_porfile(username):
    return username
